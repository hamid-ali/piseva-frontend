{
  "version": 2,
  "name": "piseva-frontend",
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_BASE_URL": "@react_app_api_base_url",
    "REACT_APP_SOCKET_URL": "@react_app_socket_url",
    "REACT_APP_ENV": "production"
  }
}